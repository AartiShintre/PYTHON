a = int(input("Enter a no"))
print("Value is :"+str(a))
print("type is : "+str(type(a)))
b = float(input("Enter a no"))
print("Value is :"+str(b))
print("type is :"+str(type(b)))
c = str(input("Enter a string"))
print("Value is :"+str(c))
print("type is :"+str(type(c)))
d = bool(input("Enter a no"))
print("value is "+str(d))
print("Value is :"+str(type(d)))
